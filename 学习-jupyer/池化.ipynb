   "source": [
    "def pool(X,pool_size=2):\n",
    "    h,w = pool_size\n",
    "    y = torch.zeros(X.shape[0] - h +1 , X.shape[1] - w + 1)\n",
    "    for i in range(h):\n",
    "        for j in range(w):\n",
    "            y[i:j] = X[i:i+h,j:j+w].max()\n",
    "            \n",
    "    return y"
   ]
