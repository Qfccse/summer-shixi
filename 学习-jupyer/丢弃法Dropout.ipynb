    "import torch\n",
    "from torch import nn\n",
    "from d2l import torch as d2l\n",
    "\n",
    "x = torch.tensor(range(10))\n",
    "\n",
    "def drop_layer(x,dropout):\n",
    "    mask = torch.rand(x.shape)\n",
    "    mask = (mask>dropout).float()\n",
    "    \n",
    "    return mask * x / (1- dropout)\n",
    "    \n",
    "drop_layer(x,0.2)"
